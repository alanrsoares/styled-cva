{
  "name": "@styled-cva/eslint-plugin",
  "version": "0.5.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test": "bun test",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write ."
  },
  "peerDependencies": {
    "eslint": "^9",
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "@eslint/js": "^9.31.0",
    "@types/eslint": "^9.6.0",
    "@types/estree": "^1.0.0",
    "@types/estree-jsx": "^1.0.5",
    "eslint": "^9.39.2",
    "prettier": "^3.4.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.30.0"
  }
}
